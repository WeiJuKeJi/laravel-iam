[
  {
    "name": "系统管理",
    "path": "/system",
    "component": "Layout",
    "redirect": "/system/users",
    "meta": {
      "title": "系统管理",
      "icon": "setting"
    },
    "children": [
      {
        "name": "用户管理",
        "path": "users",
        "component": "system/users/index",
        "meta": {
          "title": "用户管理",
          "icon": "user",
          "permissions": ["iam.users.view"]
        }
      },
      {
        "name": "角色管理",
        "path": "roles",
        "component": "system/roles/index",
        "meta": {
          "title": "角色管理",
          "icon": "peoples",
          "permissions": ["iam.roles.view"]
        }
      },
      {
        "name": "权限管理",
        "path": "permissions",
        "component": "system/permissions/index",
        "meta": {
          "title": "权限管理",
          "icon": "lock",
          "permissions": ["iam.permissions.view"]
        }
      },
      {
        "name": "菜单管理",
        "path": "menus",
        "component": "system/menus/index",
        "meta": {
          "title": "菜单管理",
          "icon": "tree-table",
          "permissions": ["iam.menus.view"]
        }
      }
    ]
  },
  {
    "name": "ExternalLink",
    "path": "//github.com",
    "meta": {
      "title": "外链示例",
      "icon": "external-link-line",
      "target": "_blank"
    },
    "is_enabled": false
  },
  {
    "name": "Iframe",
    "path": "/iframe",
    "component": "Layout",
    "meta": {
      "title": "内嵌网页",
      "icon": "window-line"
    },
    "is_enabled": false,
    "children": [
      {
        "name": "IframeView",
        "path": "view",
        "component": "/@/views/other/iframe/view.vue",
        "meta": {
          "title": "Iframe",
          "icon": "window-line",
          "dynamicNewTab": true,
          "hidden": true
        }
      },
      {
        "name": "IframeExample",
        "path": "view?url=example.com&title=示例网站",
        "meta": {
          "title": "示例网站"
        }
      }
    ]
  }
]
