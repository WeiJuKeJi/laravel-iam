[
  {
    "name": "System",
    "path": "/iam",
    "component": "Layout",
    "redirect": "/iam/user",
    "meta": {
      "title": "系统管理",
      "icon": "setting"
    },
    "children": [
      {
        "name": "User",
        "path": "user",
        "component": "iam/user/index",
        "meta": {
          "title": "用户管理",
          "icon": "user",
          "permissions": ["iam.users.view"]
        }
      },
      {
        "name": "Role",
        "path": "role",
        "component": "iam/role/index",
        "meta": {
          "title": "角色管理",
          "icon": "peoples",
          "permissions": ["iam.roles.view"]
        }
      },
      {
        "name": "Permission",
        "path": "permission",
        "component": "iam/permission/index",
        "meta": {
          "title": "权限管理",
          "icon": "lock",
          "permissions": ["iam.permissions.view"]
        }
      },
      {
        "name": "Menu",
        "path": "menu",
        "component": "iam/menu/index",
        "meta": {
          "title": "菜单管理",
          "icon": "tree-table",
          "permissions": ["iam.menus.view"]
        }
      }
    ]
  },
  {
    "name": "ExternalLink",
    "path": "//github.com",
    "meta": {
      "title": "外链示例",
      "icon": "external-link-line",
      "target": "_blank"
    },
    "is_enabled": false
  },
  {
    "name": "Iframe",
    "path": "/iframe",
    "component": "Layout",
    "meta": {
      "title": "内嵌网页",
      "icon": "window-line"
    },
    "is_enabled": false,
    "children": [
      {
        "name": "IframeView",
        "path": "view",
        "component": "/@/views/other/iframe/view.vue",
        "meta": {
          "title": "Iframe",
          "icon": "window-line",
          "dynamicNewTab": true,
          "hidden": true
        }
      },
      {
        "name": "IframeExample",
        "path": "view?url=example.com&title=示例网站",
        "meta": {
          "title": "示例网站"
        }
      }
    ]
  }
]
